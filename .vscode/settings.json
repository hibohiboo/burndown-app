{
  "editor.formatOnSave": false,
  "editor.tabSize": 2,
  "eslint.run": "onSave",
  "editor.quickSuggestions": {
    "other": true,
    "comments": true,
    "strings": true
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": false
  }
}